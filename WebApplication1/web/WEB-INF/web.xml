<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <context-param>
        <param-name>param1</param-name>
        <param-value>context-param-1</param-value>
    </context-param>
    <context-param>
        <param-name>param2</param-name>
        <param-value>context-param-2</param-value>
    </context-param>
    <listener>
        <description>ServletContextListener</description>
        <listener-class>myPkg.StartingHandler</listener-class>
    </listener>
    <servlet>
        <servlet-name>DataProvider</servlet-name>
        <servlet-class>DataProvider</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CaseStudy</servlet-name>
        <servlet-class>CaseStudy</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>RegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerifyUser</servlet-name>
        <servlet-class>VerifyUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StateAccountCreationServlet</servlet-name>
        <servlet-class>StateAccountCreationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateStateAdminProfile</servlet-name>
        <servlet-class>UpdateStateAdminProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveStateInfo</servlet-name>
        <servlet-class>saveStateInfo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EndSession</servlet-name>
        <servlet-class>EndSession</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StateDataServlet</servlet-name>
        <servlet-class>StateDataServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DataForAllState</servlet-name>
        <servlet-class>DataForAllState</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminInfo</servlet-name>
        <servlet-class>AdminInfo</servlet-class>
        <init-param>
            <param-name>adminName</param-name>
            <param-value>Utsav Nagar</param-value>
        </init-param>
        <init-param>
            <param-name>adminPhone</param-name>
            <param-value>1234567890</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>DataProvider</servlet-name>
        <url-pattern>/DataProvider</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CaseStudy</servlet-name>
        <url-pattern>/CaseStudy</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/RegistrationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerifyUser</servlet-name>
        <url-pattern>/VerifyUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StateAccountCreationServlet</servlet-name>
        <url-pattern>/StateAccountCreationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateStateAdminProfile</servlet-name>
        <url-pattern>/UpdateStateAdminProfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveStateInfo</servlet-name>
        <url-pattern>/saveStateInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EndSession</servlet-name>
        <url-pattern>/EndSession</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StateDataServlet</servlet-name>
        <url-pattern>/StateDataServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DataForAllState</servlet-name>
        <url-pattern>/DataForAllState</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminInfo</servlet-name>
        <url-pattern>/AdminInfo</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
