<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>MedicineSaveController</servlet-name>
        <servlet-class>myPkg.MedicineSaveController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MedicineSearchController</servlet-name>
        <servlet-class>myPkg.MedicineSearchController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MedicineSaveController</servlet-name>
        <url-pattern>/MedicineSaveController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MedicineSearchController</servlet-name>
        <url-pattern>/MedicineSearchController</url-pattern>
    </servlet-mapping>
</web-app>
